{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/buildServer/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\" -c",
    "test:watch": "npm test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/buildServer/build.js",
    "postbuild": "babel-node buildScripts/buildServer/distServer.js"
  },
  "author": "Dillon Christensen",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.0.0-beta.42",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "whatwg-fetch": "1.0.0",
    "whatwg-fetch-importable": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@types/express": "^4.11.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "4.19.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "3.1.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.0.7",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "mongoose": "^5.0.13",
    "nock": "8.1.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.7",
    "style-loader": "0.13.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-search-params": "^0.10.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-middleware": "3.0.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
